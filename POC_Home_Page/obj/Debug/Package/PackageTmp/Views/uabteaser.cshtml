
@inherits System.Web.Mvc.WebViewPage
<!DOCTYPE html>
@using Sitecore.Globalization
@{
    var currentLanguage = Sitecore.Context.Language.Name; // e.g., "en", "fr", "de"
}
@if(@currentLanguage=="en")
{
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>UAB — Home</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <script src="./~/media/HZTL/Scripts/splidemin.js"></script>
    <link href="./~/media/HZTL/Styles/splidemin.css" rel="stylesheet" />


    <style>
		@@font-face {
			font-family: "DINRoundPro";
			src: url("./~/media/HZTL/fonts/DINRountPro/DINRoundPro-Light.otf") format("opentype");
			font-weight: 300;
			font-style: normal;
		}

		@@font-face {
			font-family: "DINRoundPro";
			src: url("./~/media/HZTL/fonts/DINRountPro/DINRoundPro.otf") format("opentype");
			font-weight: 400;
			font-style: normal;
		}

		@@font-face {
			font-family: "DINRoundPro";
			src: url("./~/media/HZTL/fonts/DINRountPro/DINRoundPro-Medium.otf") format("opentype");
			font-weight: 500;
			font-style: normal;
		}

		@@font-face {
			font-family: "DINRoundPro";
			src: url("./~/media/HZTL/fonts/DINRountPro/DINRoundPro-Bold.otf") format("opentype");
			font-weight: 700;
			font-style: normal;
		}

		@@font-face {
			font-family: "DINRoundPro";
			src: url("./~/media/HZTL/fonts/DINRountPro/DINRoundPro-Black.otf") format("opentype");
			font-weight: 900;
			font-style: normal;
		}

		:root {
			--blue-900: #00155C;
			--blue-700: #0a1f88;
			--blue-600: #0025DD;
			--blue-100: #eaf0ff;
			--surface: #FFFFFF;
			--text: #0e1220;
			--muted: #5b6279;
			--radius-lg: 18px;
			--radius-md: 14px;
			--radius-sm: 10px;
			--shadow-md: 0 10px 24px rgba(0, 21, 92, 0.14);
			--shadow-lg: 0 16px 40px rgba(0, 21, 92, 0.16);
			--container-max: 1440px;
			--white-glass: #fff;
			--dark-blue: #00155C;
			--uab-dark-blue: #00155C;
		}

		h1,
		h2,
		h3,
		h4,
		h5,
		h6,
		p,
		span {
			margin: 0;
		}

		*,
		*::before,
		*::after {
			box-sizing: border-box;
		}

		body {
			font-family: "DINRoundPro", system-ui, -apple-system, Segoe UI, Helvetica, Arial, sans-serif;
			margin: 0;
		}

		.font-din {
			font-family: "DINRoundPro";
		}

		img {
			max-width: 100%;
			display: block;
		}

		button {
			font: inherit;
		}

		a {
			color: inherit;
			text-decoration: none;
		}

		.container,
		.container-fluid {
			max-width: var(--container-max);
			margin-inline: auto;
			padding-inline: 24px;
		}


		header.site-header {
			position: relative;
			top: 0;
			z-index: 50;
			background: transparent;
		}

		header.site-header.sticky {
			position: fixed;
			top: 0;
			z-index: 50;
			width: 100%;
			background: #fff;
			transition: all 0.3s ease-in-out;
		}

		.site-header__bar {
			display: flex;
			justify-content: space-between;
			align-items: center;
			gap: 16px;
			padding: 8px 16px 12px 16px;
			height: 104px;
		}

		.brand {
			display: inline-flex;
			align-items: center;
			gap: 8px;
			color: var(--blue-900);
			font-weight: 800;
		}

		.brand__logo__desktop {
			display: none;
		}


		.btn {
			display: flex;
			align-items: center;
			gap: 8px;
			padding: 12px 16px;
			border-radius: 100px;
			border: 0;
			cursor: pointer;
			color: #FFF;
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 12px;
			font-style: normal;
			font-weight: 700;
			line-height: 100%;
			text-transform: uppercase;
		}

		.btn--primary {
			color: #fff;

			border-radius: 100px;
			background: linear-gradient(270deg, #7A8EED -7.95%, #0025DD -0.39%);

			box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);
		}



		.btn__icon {
			width: 20px;
			height: 20px;
			display: inline-block;
			border-radius: 50%;
			color: var(--blue-600);
			display: grid;
			place-items: center;
		}

		.btn__icon svg {
			width: 16px;
			height: 16px;
			fill: var(--blue-600);
		}

		.hero {
			padding-top: 16px;
		}



		.hero__actions {
			display: flex;
			gap: 12px;
			align-items: center;
		}


		.site-footer {
			position: relative;
			padding: 48px 8px 8px 8px;
		}

		.footer__panel {
			background: linear-gradient(90deg, #BFF2FC 36%, #4DBFFE 100%);
			border-radius: 48px 48px 0 0;
			text-align: center;
		}

		.footer__social {
			display: inline-flex;
			gap: 16px;
			align-items: center;
		}

		.footer__legal {
			border-radius: 0 0 24px 24px;
			background: var(--blue-600);

			display: flex;
			flex-direction: column;
			padding: 16px 16px 24px 16px;
			justify-content: space-between;
			align-items: center;
			align-self: stretch;
		}

		.footer__legal-text {

			color: rgba(255, 255, 255, 0.80);
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 16px;
			font-style: normal;
			font-weight: 400;
			line-height: 150%;
		}

		.footer__legal-lang {
			color: rgba(255, 255, 255, 0.80);
			font-family: "DINRoundPro";
			font-size: 16px;
			font-style: normal;
			font-weight: 400;
			line-height: 150%;
		}

		.hero__card {
			display: flex;
			width: 100%;
			flex-direction: column;
			justify-content: flex-end;
			align-items: center;
			gap: 10px;
			border-radius: 24px;
			box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);
		}

		.hero__media {
			border-radius: 24px;
			height: 564px;
			width: 100%;
			box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);
			z-index: 1;

			background: url('https://placehold.co/1030x772') no-repeat center center;
			background-size: cover;
			aspect-ratio: 4/3;
		}

		.hero__content {
			display: flex;
			padding: 32px 24px;
			flex-direction: column;
			justify-content: center;
			align-items: flex-start;
			align-self: stretch;
			border-radius: 24px 24px 24px 24px;
			background: var(--White, #FFF);
			/* Blur 100 */
			backdrop-filter: blur(50px);
			margin-top: -100px;

			z-index: 2;
		}

		.hero__content h1 {
			color: var(--dark-blue);
			font-family: "DINRoundPro";
			font-size: 24px;
			font-style: normal;
			font-weight: 700;
			line-height: 140%;
			letter-spacing: -0.48px;
			text-transform: uppercase;
			margin-bottom: 8px;
		}

		.hero__content p {
			color: var(--dark-blue);
			font-family: Roboto;
			font-size: 20px;
			font-style: normal;
			font-weight: 300;
			line-height: 150%;
			margin-bottom: 16px;
		}

		.footer__brand {
			display: flex;
			flex-direction: row;
			padding: 32px 16px;
			align-items: center;
			justify-content: space-between;
		}

		.footer__brand .mobile-logo {
			display: block;
		}

		.footer__brand .desktop-logo {
			display: none;
		}

		.bg-image1 {
			height: 3450px;
			flex-shrink: 0;
			background: url('./~/media/HZTL/MBackground.png') top center repeat-y;
		}

		.bg-page {
			position: absolute;
			width: 100%;
			overflow: hidden;
			pointer-events: none;
			left: -30px;
		}

		.hero__video {
			width: 100%;
			height: 100%;
			object-fit: cover;
			border-radius: 24px;
		}

		.header-right-section {
			display: flex;
			align-items: center;
			gap: 20px;
		}

		.language-switcher-text-wrapper {
			display: flex;
			align-items: start;
			gap: 4px;
		}

		.language-switcher-text {
			color: var(--dark-blue);
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 16px;
			font-style: normal;
			font-weight: 700;
			line-height: 120%;
			text-transform: uppercase;
			cursor: pointer;
		}

		.btn--play {
			display: flex;
			padding: 8px 32px 8px 8px;
			justify-content: center;
			align-items: center;
			gap: 16px;
			border-radius: 100px;
			background: linear-gradient(270deg, #0025DD -7.95%, #00155C 0%);
			cursor: pointer;
			box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);

			color: #FFF;
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 20px;
			font-style: normal;
			font-weight: 700;
			line-height: 100%;
			text-transform: uppercase;
		}

		.btn--play span {
			width: 43px;
			height: 43px;
			flex-shrink: 0;

			fill: #fff;
		}

		.journey-progress-wrapper {
			display: flex;
			flex-direction: column;
			padding-block: 48px;
		}

		.journey-progress-label-wrapper {
			display: flex;
			flex-direction: column;
			gap: 10px;
			text-align: center;
		}

		.progress-heading {
			color: var(--dark-blue);
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 24px;
			font-style: normal;
			font-weight: 700;
			line-height: 120%;
			text-transform: uppercase;
		}

		.progress-description {
			color: var(--dark-blue);
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 20px;
			font-style: normal;
			font-weight: 300;
			line-height: 120%;
			text-transform: uppercase;
			margin-bottom: 24px;
		}

		.card {
			border-radius: 32px;
			display: flex;
			flex-direction: column;
			align-self: stretch;

			box-shadow: 0 8px 10px 0 #dcd6d6;
				margin-bottom: 28px;
				margin-right: 8px;
				margin-left: 8px;
		}

		.card img {
			width: 100%;
			height: 312px;
			object-fit: cover;

			border-radius: 32px 32px 0px 0px;
		}

		.card-body {
			display: flex;
			padding: 24px 24px 32px 24px;
			flex-direction: column;
			align-items: flex-start;
			gap: 16px;
			align-self: stretch;
			@@media (min-width: 400px) and (max-width: 768px) {
				min-height: 330px !important;
			}
			@@media (min-width: 769px) and (max-width: 1240px) {
				min-height: 370px !important;
			}
			@@media (min-width: 1241px) {
				min-height: 475px !important;
			}
		}

		.card-title {
			color: var(--UAB-Dark-Blue, #00155C);
			font-family: "DINRoundPro";
			font-size: 20px;
			font-style: normal;
			font-weight: 700;
			line-height: 120%;
			letter-spacing: -0.4px;
			text-transform: uppercase;
		}

		.card-desc {
			color: var(--UAB-Dark-Blue, #00155C);
			font-family: "DINRoundPro";
			font-size: 16px;
			font-style: normal;
			font-weight: 300;
			line-height: 120%;
		}

		/* Progress bar container */
		.progress-container {
			height: 6px;
			width: 100%;
			background: #eee;
			border-radius: 4px;
			overflow: hidden;
			cursor: pointer;
		}

		/* Progress bar itself */
		.progress-bar {
			height: 100%;
			width: 0%;
			background: #2563eb;
			/* blue */
			transition: width 0.3s ease;
		}

		.insight-progress-wrapper {
			padding: 48px 0px;
		}

		.news-card {
			padding: 0px 24px;
		}

		.news-card img {
			width: 100%;
			object-fit: cover;
			aspect-ratio: 4/3;
		}

		.news-card-body {
			padding: 20px 0px 40px 0px;

			background: var(--White-Glass, rgba(255, 255, 255, 0.80));
			backdrop-filter: blur(50px);

			justify-content: space-between;
			display: flex;
			flex-direction: column;
			gap: 16px;
		}

		.news-card-title {
			color: var(--Dark-Blue, #00155C);
			font-family: "DINRoundPro";
			font-size: 24px;
			font-style: normal;
			font-weight: 700;
			line-height: 140%;
			letter-spacing: -0.48px;
			text-transform: uppercase;
		}

		.news-card-desc {
			color: var(--UAB-Dark-Blue, #00155C);

			/* Mobile/Body/Body Big */
			font-family: Roboto;
			font-size: 16px;
			font-style: normal;
			font-weight: 300;
			line-height: 160%;

			margin-bottom: 16px;
			/* 25.6px */
		}

		.news-card-text-wrapper {
			display: flex;
			flex-direction: column;
			gap: 32px;
		}

		.news-read-more-cta {
			display: flex;
			padding: 20px 44px;
			justify-content: center;
			align-items: center;
			gap: 10px;
			align-self: stretch;

			border-radius: 100px;
			background: linear-gradient(270deg, #0025DD -7.95%, #00155C 0%);

			box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);

			color: #FFF;
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 18px;
			font-style: normal;
			font-weight: 700;
			line-height: 100%;
			/* 18px */
			text-transform: uppercase;

			width: 100%;
		}

		.custom-pagination {
			display: flex;
			gap: 12px;
			justify-content: center;
			padding-top: 40px;
		}

		.custom-pagination .dot {
			width: 72px;
			height: 4px;
			border-radius: 100px;
			background: #ccc;
			border: none;
			cursor: pointer;
		}

		.custom-pagination .dot.is-active {
			background: var(--Primary-Blue, #0025DD);
		}

		#splide2 .splide__track {
			box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);
			margin-bottom: 16px;
		}

		/** Who we are Mobile */
		.who-are-section-wrapper {
			padding-block: 48px;
		}

		.who-are-inner-section {
			display: flex;
			justify-content: center;
			padding: 28px 24px;
			flex-direction: column;
			gap: 24px;

			border-radius: 48px;
			border: 1px solid rgba(0, 21, 92, 0.08);
			background: rgba(191, 242, 252, 0.16);
			box-shadow: 4px 4px 32px 0 #FFF inset, 0 8px 32px 0 rgba(0, 21, 92, 0.08);
			backdrop-filter: blur(100px);
		}

		.who-are-wrapper {
			display: flex;
			flex-direction: column;
			gap: 8px;
		}

		.who-are-title {
			color: var(--Dark-Blue);
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 24px;
			font-style: normal;
			font-weight: 700;
			line-height: 120%;
			/* 28.8px */
			text-transform: uppercase;
		}

		.who-are-description {
			color: var(--Dark-Blue);
			text-align: center;
			font-family: "DINRoundPro";
			font-size: 20px;
			font-style: normal;
			font-weight: 300;
			line-height: 120%;
			/* 24px */
			text-transform: uppercase;
		}

		.tabs {
			border-radius: 12px;
			overflow: hidden;
			display: flex;
			flex-direction: column;
			gap: 24px;
		}

		.tab-buttons {
			display: flex;
			overflow-x: auto;
			scrollbar-width: none;
			-ms-overflow-style: none;
			box-sizing: border-box;
		}

		@@media (min-width: 1240px) {
			.tab-buttons {
				justify-content: center;
			}
		}

		.tab-buttons::-webkit-scrollbar {
			display: none;
		}

		.tab-button {
			padding: 12px 24px;
			cursor: pointer;
			border: none;
			background-color: #fff;
			color: var(--Tint-600, #A3B1F3);

			display: flex;
			flex: 1 1 0%;
			align-items: center;
			white-space: nowrap;
			min-width: max-content;

			color: rgba(0, 37, 221, 0.48);
			text-align: center;
			font-family: Roboto;
			font-size: 12px;
			font-style: normal;
			font-weight: 400;
			line-height: 160%;
			/* 19.2px */
			border-bottom: 2px solid var(--Tint-600, #A3B1F3);
		}

		.tab-button.active {
			color: var(--Primary-Blue, #0025DD);

			color: var(--UAB-Primary-Blue, #0025DD);
			text-align: center;
			font-family: Roboto;
			font-size: 12px;
			font-style: normal;
			font-weight: 400;
			line-height: 160%;
			/* 19.2px */
			border-bottom: 2px solid var(--Primary-Blue, #0025DD);
		}

		.tab-panel {
			display: none;
		}

		.tab-panel.active {
			display: block;
			animation: fadeIn 0.3s ease;
		}

		@@keyframes fadeIn {
			from {
				opacity: 0;
				transform: translateY(5px);
			}

			to {
				opacity: 1;
				transform: translateY(0);
			}
		}

		.tab-wrapper {
			display: flex;
			flex-direction: column;
			gap: 24px;
		}

		.tab-left-align {
			display: flex;
			flex-direction: column;
			align-items: flex-start;
			gap: 12px;
			flex: 1 0 0;
			align-self: stretch;
			order: 2;
		}

		.tab-right-align {
			order: 1;
		}

		.tab-right-align img {
			width: 100%;
			object-fit: cover;
			border-radius: 32px;
			height: 171px;
		}

		.tab-title {
			color: var(--UAB-Dark-Blue, #00155C);
			font-family: "DINRoundPro";
			font-size: 20px;
			font-style: normal;
			font-weight: 700;
			line-height: 120%;
			/* 24px */
			letter-spacing: -0.4px;
			text-transform: uppercase;
		}

		.tab-description {
			color: var(--UAB-Dark-Blue, #00155C);
			font-family: "DINRoundPro";
			font-size: 16px;
			font-style: normal;
			font-weight: 350;
			line-height: 120%;
			/* 19.2px */
		}

		.card-title-wrapper {
			display: flex;
			align-items: center;
			gap: 16px;
			width: 100%;
		}

		@@media (min-width: 1241px) {
			#splide1 .custom-pagination {
				display: none;
			}
		}

		@@media (min-width: 1000px) {

			.brand__logo__desktop {
				display: block;
			}

			#splide2 .splide__track {
				box-shadow: 0 8px 32px 0 rgba(0, 21, 92, 0.16);
				margin-bottom: 32px;
				border-radius: 48px;
			}

			.custom-pagination {
				display: flex;
				gap: 12px;
				justify-content: center;
				margin-top: 32px;
			}

			.custom-pagination .dot {
				width: 72px;
				height: 4px;
				border-radius: 100px;
				background: #ccc;
				border: none;
				cursor: pointer;
			}

			.custom-pagination .dot.is-active {
				background: var(--blue-600);
			}

			.insight-progress-wrapper {
				padding: 80px 128px !important;
			}

			.brand__logo__mobile {
				display: none;
			}

			.btn {
				display: flex;
				min-width: 125px;
				padding: 16px 24px 16px 32px;
				justify-content: center;
				align-items: center;
				gap: 10px;
				color: #FFF;
				text-align: center;
				font-family: "DINRoundPro";
				font-size: 16px;
				font-style: normal;
				font-weight: 700;
				line-height: 120%;
				text-transform: uppercase;
			}

			.footer__legal {
				border-radius: 0 0 48px 48px;
				background: var(--blue-600);
				flex-direction: row;
				padding: 4px 72px;
			}

			.footer__brand {
				display: flex;
				padding: 0px;
				padding: 72px;
				align-items: center;
				gap: 48px;
			}

			.footer__brand .mobile-logo {
