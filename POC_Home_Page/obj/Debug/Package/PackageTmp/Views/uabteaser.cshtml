			.card-title {
				font-size: 24px;
				letter-spacing: -0.48px;
				flex:auto;
			}

			/* Progress bar container */
			.progress-container {
				margin-top: 64px;
				height: 6px;
				width: 100%;
				background: #eee;
				border-radius: 4px;
				overflow: hidden;
				cursor: pointer;
			}

			/* Progress bar itself */
			.progress-bar {
				height: 100%;
				width: 0%;
				background: #2563eb;
				/* blue */
				transition: width 0.3s ease;
			}

			/** Who we are */
			.who-are-section-wrapper {
				padding-block: 0px;
			}

			.who-are-inner-section {
				display: flex;
				justify-content: center;
				padding: 96px;
				flex-direction: column;
				gap: 64px;

				border-radius: 48px;
				border: 1px solid rgba(0, 21, 92, 0.08);
				background: rgba(191, 242, 252, 0.16);
				box-shadow: 4px 4px 32px 0 #FFF inset, 0 8px 32px 0 rgba(0, 21, 92, 0.08);
				backdrop-filter: blur(100px);
			}

			.who-are-wrapper {
				display: flex;
				flex-direction: column;
				gap: 8px;
			}

			.who-are-title {
				color: var(--dark-blue);
				text-align: center;
				font-family: "NeoSansArabic";
				font-size: 32px;
				font-style: normal;
				font-weight: 700;
				line-height: 120%;
				text-transform: uppercase;
			}

			.who-are-description {
				color: var(--blue-700);
				text-align: center;
				font-family: "NeoSansArabic";
				font-size: 24px;
				font-style: normal;
				font-weight: 300;
				line-height: 120%;
				text-transform: uppercase;
			}

			.tabs {
				border-radius: 12px;
				overflow: hidden;
				display: flex;
				flex-direction: column;
				gap: 24px;
			}

			.tab-buttons {
				display: flex;
				overflow-x: auto;
				scrollbar-width: none;
				-ms-overflow-style: none;
				box-sizing: border-box;
			}

			@@media (min-width: 1240px) {
				.tab-buttons {
					justify-content: center;
				}
			}

			.tab-buttons::-webkit-scrollbar {
				display: none;
			}

            .tab-button {
                padding: 12px 24px;
                cursor: pointer;
                border: none;
                background-color: #fff;
                color: var(--Tint-600, #A3B1F3);
                display: flex;
                flex: none;
                align-items: center;
                white-space: nowrap;
                min-width: max-content;
                /* Desktop/Button/Sentence R */
                font-family: "NeoSansArabic";
                font-size: 24px;
                font-style: normal;
                font-weight: 400;
                line-height: 120%;
                /* 28.8px */
                letter-spacing: -0.48px;
                border-bottom: 2px solid var(--Tint-600, #A3B1F3);
            }

                .tab-button.active {
                    color: var(--Primary-Blue, #0025DD);
                    /* Desktop/Button/Sentence B */
                    font-family: "NeoSansArabic";
                    font-size: 24px;
                    font-style: normal;
                    font-weight: 700;
                    line-height: 120%;
                    /* 28.8px */
                    letter-spacing: -0.48px;
                    border-bottom: 2px solid var(--Primary-Blue, #0025DD);
                }

			.tab-content {
				padding: 20px;
			}

			.tab-panel {
				display: none;
			}

			.tab-panel.active {
				display: block;
				animation: fadeIn 0.3s ease;
			}

			@@keyframes fadeIn {
				from {
					opacity: 0;
					transform: translateY(5px);
				}

				to {
					opacity: 1;
					transform: translateY(0);
				}
			}

			.tab-wrapper {
				display: flex;
				gap: 24px;
				flex-direction: row;
			}

			.tab-left-align {
				display: flex;
				padding: 32px 0;
				flex-direction: column;
				align-items: flex-start;
				gap: 10px;
				flex: 1 0 0;
				align-self: stretch;
				width: 50%;
				order: 1;
			}

			.tab-right-align {
				width: 50%;
				order: 2;
			}

			.tab-right-align img {
				width: 100%;
				object-fit: cover;
				border-radius: 32px;
				height: 100%;
			}

			.tab-title {
				color: var(--UAB-Dark-Blue, #00155C);
				font-family: "NeoSansArabic";
				font-size: 24px;
				font-style: normal;
				font-weight: 700;
				line-height: 120%;
				/* 28.8px */
				letter-spacing: -0.48px;
				text-transform: uppercase;
			}

			.tab-description {
				color: #00155C;

				/* Desktop/Body/Body Base */
				font-family: Roboto;
				font-size: 16px;
				font-style: normal;
				font-weight: 300;
				line-height: 150%;
				/* 24px */
			}
		}

		/* Modal Styles */
		.modal {
			display: none;
			position: fixed;
			z-index: 1000;
			left: 0;
			top: 0;
			width: 100%;
			height: 100%;
			background: rgba(0, 21, 92, 0.08);
			backdrop-filter: blur(16px);
		}

		.modal-content {
			position: relative;
			margin: 5% auto;
			width: 90%;
			max-width: 1200px;
			height: 80vh;
			max-height: 800px;
			background: #fff;
			border-radius: 32px;
			overflow: hidden;
			background-blend-mode: normal, normal, multiply, normal;
			box-shadow: 0 6.4px 25.6px 0 rgba(0, 21, 92, 0.16);
			display: flex;
			flex-direction: column;
		}

		.modal-close {
			position: absolute;
			top: 15px;
			right: 20px;
			cursor: pointer;
			z-index: 9999;
			width: 80px;
			height: 80px;
			border-radius: 50%;
			display: flex;
			justify-content: center;
			transition: background-color 0.3s ease;
			align-items: center;
			border-radius: 100px;
			background: rgba(0, 0, 0, 0.30);
			backdrop-filter: blur(20px);
		}

		.modal-video {
			width: 100%;
			height: 100%;
			display: block;
			object-fit: cover;
			background: #000;
		}

		/* Video controls styling for rounded modal */
		.modal-video::-webkit-media-controls-panel {
			border-radius: 0 0 32px 32px;
			padding: 10px;
		}

		/* Timeline specific styling */
		.modal-video::-webkit-media-controls-timeline {
			border-radius: 8px;
			margin: 10px;
			padding: 4px 4px;
			width: 100%;
			max-width: 97%;
			height: 6px;
		}

		.modal-video::-webkit-media-controls-play-button,
		.modal-video::-webkit-media-controls-volume-slider {
			border-radius: 8px;
		}

		.modal-video::-webkit-media-controls-fullscreen-button {
			border-radius: 8px;
		}

		/* Firefox video controls */
		.modal-video::-moz-media-controls {
			background: linear-gradient(transparent, rgba(0, 0, 0, 0.7));
			border-radius: 0 0 32px 32px;
			padding: 10px;
		}

		@@media (max-width: 768px) {
			.modal-content {
				margin: 2% auto;
				width: 95%;
				height: 90vh;
				max-height: none;
			}

			.modal-video {
				height: 100%;
				object-fit: cover;
			}

			/* Mobile video controls styling */
			.modal-video::-webkit-media-controls-panel {
				border-radius: 0 0 32px 32px;
				padding: 8px;
			}

			/* Mobile timeline specific styling */
			.modal-video::-webkit-media-controls-timeline {
				border-radius: 8px;
				margin: 6px 12px;
				padding: 3px 6px;
				width: 100%;
				max-width: 250px;
				height: 5px;
			}

			.modal-video::-moz-media-controls {
				border-radius: 0 0 32px 32px;
				padding: 8px;
			}

			.modal-close {
				position: absolute;
				top: 15px;
				right: 20px;
				cursor: pointer;
				z-index: 9999;
				width: 40px;
				height: 40px;
				border-radius: 50%;
				display: flex;
				justify-content: center;
				transition: background-color 0.3s ease;
				align-items: center;
				border-radius: 100px;
				background: rgba(0, 0, 0, 0.30);
				backdrop-filter: blur(20px);

				svg {
					width: 24px;
					height: 24px;
				}
			}
		}
    </style>
</head>

