    <script>
        document.addEventListener('DOMContentLoaded', function () {


            const tabButtons = document.querySelectorAll('.tab-button');
            const tabPanels = document.querySelectorAll('.tab-panel');

            tabButtons.forEach((button) => {
                button.addEventListener('click', () => {
                    // Remove active classes
                    tabButtons.forEach(btn => btn.classList.remove('active'));
                    tabPanels.forEach(panel => panel.classList.remove('active'));

                    // Add active class to clicked button
                    button.classList.add('active');

                    // Show the corresponding tab panel
                    const target = document.getElementById(button.dataset.tab);
                    target.classList.add('active');
                });
            });

            // Video Modal functionality
            var modal = document.getElementById('videoModal');
            var playButton = document.querySelector('.btn--play');
            var closeButton = document.querySelector('.modal-close');
            var modalVideo = document.querySelector('.modal-video');

            // Open modal when play button is clicked
            playButton.addEventListener('click', function () {
                modal.style.display = 'block';
                document.body.style.overflow = 'hidden'; // Prevent background scrolling
                // Play video when modal opens
                modalVideo.play();
            });

            // Close modal when close button is clicked
            closeButton.addEventListener('click', function () {
                modal.style.display = 'none';
                document.body.style.overflow = 'auto'; // Restore scrolling
                // Pause video when modal closes
                modalVideo.pause();
                modalVideo.currentTime = 0; // Reset video to beginning
            });

            // Close modal when clicking outside the video
            modal.addEventListener('click', function (event) {
                if (event.target === modal) {
                    modal.style.display = 'none';
                    document.body.style.overflow = 'auto';
                    modalVideo.pause();
                    modalVideo.currentTime = 0;
                }
            });

            // Close modal with Escape key
            document.addEventListener('keydown', function (event) {
                if (event.key === 'Escape' && modal.style.display === 'block') {
                    modal.style.display = 'none';
                    document.body.style.overflow = 'auto';
                    modalVideo.pause();
                    modalVideo.currentTime = 0;
                }
            });

            // sticky header
            window.addEventListener('scroll', function () {
                var header = document.querySelector('.site-header');
                header.classList.toggle('sticky', window.scrollY > 50);
            });

            var splide = new Splide('#splide1', {
                type: "loop",
                perPage: 4,
                gap: '16px',
                perMove: 1,
                autoplay: false,
                arrows: false,
                pagination: false,
                lazyLoad: 'nearby',
                direction: 'rtl',
                breakpoints: {
                    1440: { perPage: 4, gap: '16px' },
                    1241: { gap: "16px", perPage: 2.5, gap: '16px' },
                    768: { gap: "16px", perMove: 1, perPage: 1.5 },
                    450: { gap: "16px", perMove: 1, perPage: 1.2 }
                },
            });



            splide.mount();


            var splide2 = new Splide('#splide2', {
                type: "loop",
                perPage: 1,
                perMove: 1,
                autoplay: false,
                arrows: false,
                pagination: false,
                lazyLoad: 'nearby',
                direction: 'rtl',
            });

            splide2.mount();

            // Generate custom pagination dynamically
            var paginationContainer = document.querySelector('#splide2 .custom-pagination');
            var dots = [];

            for (let i = 0; i < splide2.length; i++) {
                let dot = document.createElement('button');
                dot.className = 'dot' + (i === 0 ? ' is-active' : '');
                dot.addEventListener('click', () => splide2.go(i));
                paginationContainer.appendChild(dot);
                dots.push(dot);
            }

            // Update active state when carousel moves
            splide2.on('move', function (newIndex) {
                dots.forEach(dot => dot.classList.remove('is-active'));
                dots[newIndex].classList.add('is-active');
            });

            // Generate custom pagination dynamically
            var paginationContainer1 = document.querySelector('#splide1 .custom-pagination');
            var dots1 = [];

            for (let i = 0; i < splide.length; i++) {
                let dot1 = document.createElement('button');
                dot1.className = 'dot' + (i === 0 ? ' is-active' : '');
                dot1.addEventListener('click', () => splide.go(i));
                paginationContainer1.appendChild(dot1);
                dots1.push(dot1);
            }

            // Update active state when carousel moves
            splide.on('move', function (newIndex) {
                dots1.forEach(dot1 => dot1.classList.remove('is-active'));
                dots1[newIndex].classList.add('is-active');
            });
        });
    </script>
